{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "properties": {
                        "cache_duration": "none",
                        "folder_separator_icon": "/",
                        "style": "full"
                    },
                    "template": "\uf07b {{ .Path }} ",
                    "foreground": "#45F1C2",
                    "type": "path",
                    "style": "plain"
                },
                {
                    "properties": {
                        "cache_duration": "none",
                        "fetch_stash_count": true,
                        "fetch_upstream_icon": true
                    },
                    "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
                    "foreground": "#14A5AE",
                    "powerline_symbol": "\ue0b0",
                    "type": "git",
                    "style": "plain"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "properties": {
                        "cache_duration": "none"
                    },
                    "template": "\ue3bf ",
                    "foreground": "#cd5e42",
                    "type": "root",
                    "style": "plain"
                },
                {
                    "properties": {
                        "cache_duration": "none"
                    },
                    "template": "Î» ",
                    "foreground": "#999999",
                    "type": "text",
                    "style": "plain"
                }
            ]
        }
    ],
    "version": 3
}
